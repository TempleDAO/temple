{
  "name": "@temple/dapp",
  "version": "0.1.1",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "prepare": "yarn typechain:compile && yarn typechain:copy",
    "typechain:compile": "cd ../../protocol && yarn compile --force",
    "typechain:copy": "cp -r ../../protocol/typechain ./src/types",
    "dev": "vite",
    "compile": "tsc",
    "vite:build": "vite build",
    "build": "yarn prepare && tsc && vite build",
    "vercel-build": "yarn prepare && tsc && vite build",
    "serve": "vite preview",
    "version:release": "standard-version",
    "version:prerelease": "standard-version --prerelease beta",
    "build-with-stats": "tsc && BUILD_STATS=true vite build"
  },
  "dependencies": {
    "@balancer-labs/sdk": "^1.0.4",
    "@tippyjs/react": "^4.2.6",
    "axios": "^0.24.0",
    "buffer": "^6.0.3",
    "d3-shape": "^3.0.1",
    "date-fns": "^2.28.0",
    "ethers": "5.7.0",
    "events": "^3.3.0",
    "howler": "^2.2.3",
    "lodash": "^4.17.21",
    "lottie-web": "^5.9.4",
    "millify": "^4.0.0",
    "polished": "^4.1.3",
    "posthog-js": "^1.28.0",
    "process": "^0.11.10",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-responsive": "^9.0.0-beta.5",
    "react-router-dom": "^6.0.2",
    "react-select": "^4.3.1",
    "react-typical": "^0.1.3",
    "react-vis": "^1.11.7",
    "recharts": "^2.4.3",
    "styled-components": "^5.3.3",
    "tippy.js": "^6.3.7",
    "use-debounce": "^9.0.2",
    "use-interval": "^1.4.0",
    "util": "^0.12.4",
    "wagmi": "^0.11.6"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nomiclabs/hardhat-ethers": "2.2.1",
    "@synthetixio/synpress": "^1.2.0",
    "@typechain/ethers-v5": "10.1.0",
    "@types/babel__core": "^7.1.16",
    "@types/d3-shape": "^3.0.2",
    "@types/dateformat": "^5.0.0",
    "@types/howler": "^2.2.4",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-select": "^4.0.18",
    "@types/react-vis": "^1.11.10",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-legacy": "^1.6.3",
    "@vitejs/plugin-react": "^1.3.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "env-cmd": "^10.1.0",
    "eslint": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "lint-staged": ">=10",
    "prettier": "^2.5.0",
    "standard-version": "^9.3.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4",
    "vite": "^2.7.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css}": [
      "prettier --write"
    ]
  }
}
