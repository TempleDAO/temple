{
  "name": "@temple/core-subgraph",
  "version": "0.1.0",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --product hosted-service templedao/templedao-core",
    "create-local": "graph create --node http://localhost:8020/ templedao-core",
    "remove-local": "graph remove --node http://localhost:8020/ templedao-core",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 templedao-core",
    "local-node": "echo foo",
    "local-graph-setup": "./scripts/graph-node-setup.sh",
    "local-graph-start": "cd graph-node/docker && docker compose up",
    "local-graph-stop": "cd graph-node/docker && docker compose down",
    "local-ganache": "ganache -i=1337 --db=./.data -d"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.28.1",
    "@graphprotocol/graph-ts": "^0.26.0"
  }
}
